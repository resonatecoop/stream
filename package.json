{
  "name": "stream2own",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "audit": "npm run audit:npm --workspaces --if-present",
    "dev": "npm run dev --workspace beta-player",
    "dev:embed": "npm run dev --workspace embed",
    "test": "npm run test --workspaces --if-present",
    "publish": "lerna publish from-package",
    "start": "npm run start --workspace beta-player",
    "start:embed": "npm run start --workspace @resonate/player-embed",
    "build": "npm run build --workspace beta-player",
    "build:embed": "npm run build --workspace @resonate/player-embed"
  },
  "workspaces": [
    "packages/*",
    "embed",
    "beta"
  ],
  "standard": {
    "ignore": [
      "**/lib/**"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@tap-format/spec": "^0.2.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "dependency-check": "^4.1.0",
    "dotenv-safe": "^8.2.0",
    "hstream": "^1.2.0",
    "jalla": "1.0.0-38",
    "koa-mount": "^4.0.0",
    "postcss-custom-media": "^7.0.7",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-reporter": "^6.0.1",
    "sheetify-postcss": "^2.0.0",
    "standard": "^16.0.4",
    "tape": "^5.3.1",
    "tape-run": "^9.0.0"
  }
}
