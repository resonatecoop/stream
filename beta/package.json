{
  "name": "beta-player",
  "main": "src/index.js",
  "version": "7.0.0-2",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production cd src && jalla build index.js --dir ../dist --sw sw.js",
    "dev": "cd src && jalla start index.js --watch --sw sw.js",
    "audit:npm": "npm audit",
    "start": "NODE_ENV=production jalla serve index.js --dir dist --sw sw.js",
    "test": "standard && npm run test:deps",
    "test:deps": "dependency-check ./src/index.js -i sheetify -i @resonate/tachyons --no-dev",
    "test:browser": "browserify src/test/browser/index.js | tape-run | tap-format-spec"
  },
  "repository": "https://github.com/resonatecoop/stream2own",
  "documentify": {
    "transform": [
      "./documentify/variables",
      "./documentify/utf8"
    ]
  },
  "browserify": {
    "transform": [
      [
        "@resonate/envlocalify",
        {
          "global": true
        }
      ]
    ]
  },
  "browserslist": [
    "last 1 version",
    ">1%",
    "not dead"
  ],
  "sheetify": {
    "transform": [
      [
        "sheetify-postcss",
        {
          "plugins": [
            [
              "postcss-import"
            ],
            [
              "postcss-preset-env",
              {
                "stage": 1,
                "features": {
                  "nesting-rules": true
                }
              }
            ],
            [
              "postcss-custom-media"
            ]
          ]
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {
        "stage": 1,
        "features": {
          "nesting-rules": true
        }
      },
      "postcss-custom-media": {}
    }
  },
  "standard": {
    "ignore": [
      "/dist/**/*.js"
    ]
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@resonate/api-factory-generator": "*",
    "@resonate/api-service": "*",
    "@resonate/button": "*",
    "@resonate/button-component": "*",
    "@resonate/choo-plugins": "*",
    "@resonate/counter": "*",
    "@resonate/dialog-component": "*",
    "@resonate/icon-element": "*",
    "@resonate/input-element": "*",
    "@resonate/link-element": "*",
    "@resonate/menu-button-options-component": "*",
    "@resonate/play-count": "*",
    "@resonate/play-count-component": "*",
    "@resonate/player-component": "*",
    "@resonate/playlist-component": "*",
    "@resonate/search-component": "*",
    "@resonate/svg-image-placeholder": "*",
    "@resonate/tachyons": "*",
    "@resonate/textarea-element": "*",
    "@resonate/theme-skins": "*",
    "@resonate/track-component": "*",
    "@resonate/utils": "*",
    "@stripe/stripe-js": "^1.13.2",
    "browser-cookies": "^1.2.0",
    "browser-or-node": "^1.1.0",
    "chartist": "^0.11.4",
    "chartist-logaxis": "^1.3.1",
    "choo": "7.1.0",
    "choo-devtools": "^3.0.2",
    "choo-meta": "^2.0.0",
    "choo-notification": "^1.0.0",
    "choo-service-worker": "^2.4.0",
    "clipboard-copy": "^3.0.0",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.16.1",
    "is-equal-shallow": "^0.1.3",
    "nanocomponent": "^6.5.1",
    "nanohtml": "^1.9.1",
    "nanologger": "^1.3.1",
    "nanomorph": "^5.1.3",
    "nanostate": "^1.2.1",
    "shallow-clone": "^3.0.0",
    "sheetify": "^8.0.0",
    "validate-formdata": "^2.1.0",
    "validator": "13.7.0",
    "web-animations-js": "^2.3.2"
  }
}
